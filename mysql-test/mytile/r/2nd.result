# INT and VARCHAR - GREEK
CREATE TABLE t1 (
column1 varchar(255),
column2 int dimension=1 lower_bound="0" upper_bound="100" tile_extent="10" NOT NULL
) ENGINE=mytile;
INSERT INTO t1 (column1, column2) VALUES ("δοκιμή", 1);
select column1, column2 FROM t1;
column1	column2
δοκιμή	1
DROP TABLE t1;
