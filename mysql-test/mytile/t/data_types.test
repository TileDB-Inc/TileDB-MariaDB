--echo # INTEGER
CREATE TABLE t1 (
  dim1 integer dimension=1 lower_bound="0" upper_bound="100" tile_extent="10",
  attr1 integer
) ENGINE=mytile;
INSERT INTO t1 VALUES (1,0);
select * FROM t1;
DROP TABLE t1;

